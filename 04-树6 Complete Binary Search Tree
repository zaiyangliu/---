#include<cstdio>
#include<algorithm>
using namespace std;
const int maxn = 1010;
int n, number[maxn], cbt[maxn], index = 0;
bool cmp(int a, int b){
    return a < b;
}
void inOrder(int root){
    if(root > n) return;
    inOrder(2*root);
    cbt[root] = number[index++];
    inOrder(2*root + 1);
}
int main(){
    scanf("%d", &n);
    for(int i = 0; i < n; ++i){
        scanf("%d", &number[i]);
    }
    sort(number, number+n, cmp);//sort函数默认是从小到大排序，可以不用cmp
    inOrder(1);
    for(int i = 1; i <= n; ++i){
        printf("%d", cbt[i]);
        if(i < n) printf(" ");
    }
    return 0;
}
